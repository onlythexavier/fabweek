@model ISEN.DotNet.Library.Models.Equipment

@{
    ViewBag.Title = "Enrole Equipment";
    Layout = "_Layout";
}

<h2>New Equipment</h2>
<form asp-action="Enrole">
    <div class="form-horizontal">
        <hr />
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <label asp-for="Name" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="IdObject" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="IdObject" class="form-control" />
                <span asp-validation-for="IdObject" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
</form>

<div>
    <a asp-controller="Owner" asp-action="MyEquipment" class="btn btn-default">Back to the Future</a>
</div>